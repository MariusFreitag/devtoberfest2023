                <Property Name="CountryCode_code" Type="Edm.String" MaxLength="3"/>
                <Property Name="PhoneNumber" Type="Edm.String" MaxLength="30"/>
                <Property Name="EMailAddress" Type="Edm.String" MaxLength="256"/>
