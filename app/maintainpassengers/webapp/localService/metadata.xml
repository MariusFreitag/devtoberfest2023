                <Property Name="createdBy" Type="Edm.String" MaxLength="255"/>
                <Property Name="LastChangedAt" Type="Edm.DateTimeOffset" Precision="7"/>
                <Property Name="LastChangedBy" Type="Edm.String" MaxLength="255"/>
